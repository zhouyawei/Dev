<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_IChat" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://127.0.0.1:18888/ChatService" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IChat" contract="WCFChat.IChat"
                name="NetTcpBinding_IChat">
                <identity>
                    <userPrincipalName value="admin-PC\Administrator" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>